/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "experimentalDecorators": true,
    "types": [],
    "baseUrl": "./",
    "paths": {
      "@myapp/*":["src/app/*"],
      "@myenvironments/*":["src/environments/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
